class Solution {
    public int countOdds(int low, int high) {
        int count = 0;
        int start;
        if(low % 2 == 1)
            start = low;
        else
            start = low + 1;
        
        while(start <= high){
            start += 2;
            ++count;
        }
       
        return count;
    }
}
